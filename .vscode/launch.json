{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "args" : ["."],
      "outputCapture": "std",
    },
    {
      "name": "Test",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "test"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "smartStep": true,
      "sourceMaps": true,
      
      
      "outputCapture": "std",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "JASMINE_TIMEOUT": "3600000",
        "TIPC_CHECK_CONNECTION_TIMEOUT": "false"
      }
    },
    {
      "name": "Test this file",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "test",
        "${file}"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "smartStep": true,
      "sourceMaps": true,
      
      
      "outputCapture": "std",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "JASMINE_TIMEOUT": "3600000",
        "TIPC_CHECK_CONNECTION_TIMEOUT": "false"
      }
    }
  ],
  "compounds": []
}