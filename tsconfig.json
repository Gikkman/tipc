      "outDir": "_compile",
      "module": "commonjs",
      "target": "es6",
      "allowJs": false,
      "sourceMap": true,
      "baseUrl": "./electron",
      "paths": {
          "@shared/*": ["shared/*"]
        },
      "strict": true

  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
      "electron/**/*"
  ]
}